l>\n          <li v-for=\"(v,i) in listed_movie_item\" :key=\"i\"> {{v}}</li>\n        </ul>\n        <!-- <pre class=\"language-json\"><code>{{ value  }}</code></pre> -->\n      </div>\n\n      <div class=\"input-group\">\n\n        <!-- <input type=\"text\" class=\"form-control\" v-model=\"search\" id=\"auto_complete\" @input=\"onChange\"  @keyup.enter=\"auto_complete()\" placeholder=\"address...\"> -->\n      \n      <!--   <div class=\"input-group-append\">\n          <button class=\"btn btn-primary\" id=\"map_btn\"><i class=\"fa fa-search\"></i></button>\n        </div> -->\n      </div>\n     <!--  <ul>\n        <li v-for=\"(v,i) in item_autocomplete\" class=\"autocomplete-result\" :key=\"i\"\n         @click=\"setItem(v.title)\" >\n              {{ v.title }}\n        </li>\n      </ul> -->\n    </form>\n    <div id=\"map\" style=\"height:300px;\">\n    </div>\n\n  </div>\n</template>\n<script src=\"https://unpkg.com/vue-suggestion\"></script>\n  <script src='//cdn.jsdelivr.net/gmaps4rails/2.1.2/gmaps4rails.js'> </script>\n  <script src='//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore.js'> </script>\n  <script src='https://cdn.jsdelivr.net/npm/vue-lodash@2.1.1/dist/bundle.min.js'> </script>\n\n<script>\n import Multiselect from 'vue-multiselect'\n\nexport default {\n    components: { Multiselect},\n  data() {\n    return {\n     selected_movie: '',\n      movies: [],\n      movie_lists: [],\n      listed_movie_item:[],\n      item_autocomplete: [],\n       results_copy: [],\n       isOpen:false,\n    };\n  },\n  mounted() {\n\n    var map = new GMaps({\n      el: '#map',\n      lat: -12.043333,\n      lng: -77.028333\n    });\n  \n  },\n\n  created() {\n\n    this.getMovieList();\n    // this.display_scenes_near(lat, lng);\n  },\n\n  methods: {\n    customLabelForMovies (option ) {\n     return `${option.title}`\n    },\n\n\n   getMovieList(){\n      let that =this;\n         that.axios.get('https://data.sfgov.org/resource/yitu-d5am.json').then(response => {\n          that.movie_lists = response.data;\n\n          }).catch(err => {\n       });\n    },\n\n    asyncFindMovie(query){\n      let that = this;\n\n      if (query.length > 2) {\n\n        var filtered_item = this.movie_lists.filter(o => {\n              return o.title.toLowerCase().includes(query.toLowerCase());\n          });\n\n        that.movies = that._.uniqBy(filtered_item,'title');\n\n\n      }else{\n         // that.movies =[];\n      }\n\n    },\n    selectMovie(){\n      let that =this;\n\n      that.listed_movie_item =[];\n\n          setTimeout(function(){\n      if (that.selected_movie != null || that.selected_movie == '') {\n\n             that.listed_movie_item = that.movie_lists.filter(o => {\n                    return o.title == that.selected_movie.title;\n                });\n\n          that.display_scenes_near(that.selected_movie.locations);\n\n\n         }\n      },0);\n\n    },\n\n    display_scenes_near(city) {\n\n      let that =this;\n\n      that.axios.get(\"https://data.sfgov.org/resource/yitu-d5am.json?locations=\"+city+\"&key=AIzaSyBTGnKT7dt597vo9QgeQ7BFhvSRP4eiMSM\").then(response => {\n        console.log(response);\n          // that.movie_lists = response.data;\n\n          }).catch(err => {\n       });\n\n      // $.ajax({\n      //   url: \"https://data.sfgov.org/resource/yitu-d5am.json/\" + \"near?lat=\" + lat + \"&lng=\" + lng,\n      //   jsonp: \"callback\",\n      //   dataType: \"jsonp\",\n\n      //   success: function( scenes ) {\n      //     console.log(scenes);\n      //   }\n      // });\n    }\n\n  \n  },\n};\n</script>\n\n<style src=\"vue-multiselect/dist/vue-multiselect.min.css\"></style>\n\n<style type=\"text/css\">\n #map {\n  width: 400px;\n  height: 400px;\n}\n</style>\n"]}]}